{
    "openapi": "3.0.0",
    "info": {
        "title": "Scilab API",
        "description": "API for Scilab module from virtual laboratory",
        "contact": {
            "name": "xvrbovskya@stuba.sk"
        },
        "version": "1.0"
    },
    "paths": {
        "/api/auth/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register user",
                "description": "Registers new user",
                "operationId": "c0eaa9600f14ac767e20818010954c59",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "description": "Full name",
                                        "type": "string",
                                        "example": "Test"
                                    },
                                    "email": {
                                        "description": "Email address",
                                        "type": "string",
                                        "example": "test@example.com"
                                    },
                                    "password": {
                                        "description": "Password",
                                        "type": "string",
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "User Created Successfully"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "9|iwvR6QMeX1s8a83wZFhxvApSbgu7Ee8CJs9N7nR4b05f26e3"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login user",
                "description": "Login user",
                "operationId": "e45287a3dc02f4633476dbf218bb9ef7",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "description": "Email address",
                                        "type": "string",
                                        "example": "test@example.com"
                                    },
                                    "password": {
                                        "description": "Password",
                                        "type": "string",
                                        "example": "password"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "User Logged In Successfully"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "9|iwvR6QMeX1s8a83wZFhxvApSbgu7Ee8CJs9N7nR4b05f26e3"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Wrong credentials",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Wrong Email or Password"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/experiments": {
            "get": {
                "tags": [
                    "Experiments"
                ],
                "summary": "Get a list of experiments",
                "description": "Returns all experiments",
                "operationId": "a0534edd21fd0b243acea4da324c8610",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "experiments": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "file_name": {
                                                        "type": "string",
                                                        "example": "experiment_files/1622619815_1619954846_tcn.zcos"
                                                    },
                                                    "context": {
                                                        "type": "string",
                                                        "example": "{}"
                                                    },
                                                    "output": {
                                                        "type": "string",
                                                        "example": "{}"
                                                    },
                                                    "save": {
                                                        "type": "integer",
                                                        "example": 0
                                                    },
                                                    "created_by": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "format": "date-time"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "format": "date-time"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Experiments"
                ],
                "summary": "Create a new experiment",
                "description": "Stores a new experiment",
                "operationId": "c313fedf97b93b728e83282661ec8f70",
                "parameters": [
                    {
                        "name": "Accept",
                        "in": "header",
                        "description": "Authentication token",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "default": "application/json"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "file",
                                    "context",
                                    "output",
                                    "save"
                                ],
                                "properties": {
                                    "file": {
                                        "description": "Experiment file",
                                        "type": "string",
                                        "format": "binary"
                                    },
                                    "context": {
                                        "description": "Context",
                                        "type": "string"
                                    },
                                    "output": {
                                        "description": "Output",
                                        "type": "string"
                                    },
                                    "save": {
                                        "description": "Save",
                                        "type": "integer",
                                        "enum": [
                                            0,
                                            1
                                        ]
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Experiment created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "simulation": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "time": {
                                                        "type": "number"
                                                    },
                                                    "height": {
                                                        "type": "number"
                                                    },
                                                    "velocity": {
                                                        "type": "number"
                                                    }
                                                },
                                                "type": "object"
                                            },
                                            "example": [
                                                {
                                                    "time": 0,
                                                    "height": 80,
                                                    "velocity": 0
                                                },
                                                {
                                                    "time": 0.1,
                                                    "height": 80,
                                                    "velocity": 0.4801576
                                                }
                                            ]
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid input",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "errors": {
                                            "properties": {
                                                "file": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "The file field is required."
                                                    }
                                                },
                                                "context": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "The context field is required."
                                                    }
                                                },
                                                "output": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "The output field is required."
                                                    }
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/experiments/{id}": {
            "get": {
                "tags": [
                    "Experiments"
                ],
                "summary": "Get an experiment by ID",
                "description": "Returns an experiment based on ID",
                "operationId": "dcb8183b23f8500f17e3f9fec88e1feb",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the experiment",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": 1
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "file_name": {
                                            "type": "string",
                                            "example": "experiment_files/1622619815_1619954846_tcn.zcos"
                                        },
                                        "context": {
                                            "type": "string",
                                            "example": "{}"
                                        },
                                        "output": {
                                            "type": "string",
                                            "example": "{}"
                                        },
                                        "save": {
                                            "type": "integer",
                                            "example": 0
                                        },
                                        "created_by": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "format": "date-time"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid ID supplied",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Invalid id."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Experiment not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "There is no experiment with that id."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Experiments"
                ],
                "summary": "Delete an experiment by ID",
                "description": "Deletes an experiment based on ID",
                "operationId": "c7759017f9e8eea4653a3adef4c71a29",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of the experiment",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": 1
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Experiment deleted successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Experiment deleted successfully"
                                        },
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid ID supplied",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Invalid id."
                                        },
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Experiment not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "There is no experiment with that id."
                                        },
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "Auth",
            "description": "API Endpoints for authentication"
        }
    ]
}